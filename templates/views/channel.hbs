    <style>
        body {
            background-color: #fffbed;
        }

        h1 {
            text-shadow: 1px 1.5px black;
        }

        .header {
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        .logo {
            display: flex;
            align-items: center;
        }

        .logo img {
            width: 50px;
            /* Adjust the size as needed */
            height: auto;
            /* Maintain aspect ratio */
            margin-right: 10px;
            /* Add spacing between logo and heading */
        }

        ul {
            list-style-type: none;
            padding: 0;
        }

        ul li {
            display: inline;
            margin-right: 20px;
        }

        h2 {
            text-align: center;
        }

        table {
            text-align: center;
            margin: 0 auto;
        }

        table,
        tr,
        td {
            border: 2px solid black;
            border-collapse: collapse;
            padding: 5px;
            justify-content: center;

        }

        p {
            font-family: 'Titillium Web', sans-serif;
        }

        #gateway h2 {
            text-align: center;
        }
        .footer {
            background-color: #333333d6;
            color: #fff;
            padding: 20px;
            text-align: center;
            border-radius: 7px;
        }

        .footer a {
            color: #fff;
            text-decoration: none;
            margin: 0 10px;
        }
    </style>
</head>

